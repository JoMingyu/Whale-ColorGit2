{
    "manifest_version": 2,
    
    "name": "GitPainter",
    "description": "GitHub 프로필에 색을 입히기 위한 프로그램",
    "version": "1.0",
    
    "browser_action": {
        "default_icon": "icon_temp.png",
        "default_popup": "popup.html",
        "default_title": "GitPainter"
    },
    "permissions": [
        "activeTab",
        "<all_urls>"
    ]
}
